# WordPress Theme .gitignore
# Ignore OS files
.DS_Store
Thumbs.db

# Ignore log files and databases
*.log
*.sql
*.sqlite

# Ignore node modules and build artifacts
node_modules/
dist/
build/

# Ignore compiled CSS/JS if you use preprocessors
*.css.map
*.js.map

# Ignore uploads and cache
uploads/
.cache/

# Ignore IDE/project files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore backup files
*.bak
*.tmp

# Ignore composer dependencies
vendor/

# Ignore environment files
.env
.env.*

# Ignore local keys
kilikey
kilikey.pub
*.pem
*.key

# Ignore npm package lock
package-lock.json
yarn.lock

# Ignore SASS/LESS cache
.sass-cache/
.cache/

# Ignore PHPStorm
.phpstorm.meta.php
